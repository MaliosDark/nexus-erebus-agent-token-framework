{
  "name": "nexus-erebus-agent-token-framework",
  "version": "2.6.0",
  "description": "Autonomous Solana agents that interact via Twitter/Telegram, powered by Ollama and NXR token mechanics.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "prestart": "sh -c \"./redis/src/redis-server --daemonize yes\"",
    "serve:agent": "node ensure-deps.js && node check-env.js && node index.js",
    "serve:api": "node api-server.js",
    "start": "concurrently -k -s first --raw \"npm:serve:agent\" \"npm:serve:api\"",
    "check": "node check-env.js",
    "dev": "NODE_ENV=development concurrently -k -s first --raw \"npm:serve:agent\" \"npm:serve:api\""
  },
  "keywords": [
    "solana",
    "nxr",
    "nexus",
    "erebus",
    "bot",
    "telegram",
    "twitter",
    "crypto",
    "agent",
    "trading",
    "ollama"
  ],
  "author": {
    "name": "Andryu Schittone",
    "email": "malios666@gmail.com",
    "url": "https://github.com/MaliosDark"
  },
  "license": "MIT",
  "dependencies": {
    "@solana/spl-token": "^0.4.13",
    "@solana/web3.js": "^1.98.2",
    "agent-twitter-client": "file:agent-twitter-client",
    "bullmq": "^5.52.1",
    "concurrently": "^8.2.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csurf": "^1.10.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-csp-header": "^6.1.0",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.5.0",
    "helmet": "^7.2.0",
    "hpp": "^0.2.3",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "lodash-id": "^0.14.1",
    "lowdb": "^5.1.0",
    "morgan": "^1.10.0",
    "nanoid": "^5.1.5",
    "node-fetch": "^3.3.2",
    "prom-client": "^15.1.3",
    "rate-limit-redis": "^4.2.0",
    "telegraf": "^4.16.3",
    "tough-cookie": "^5.1.2",
    "ulid": "^3.0.0",
    "winston": "^3.17.0",
    "xss-clean": "^0.1.4",
    "zod": "^3.24.3"
  }
}
